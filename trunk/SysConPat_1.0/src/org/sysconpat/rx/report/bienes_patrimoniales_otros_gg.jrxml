	<queryString>
		<![CDATA[SELECT
     upper(LOC.nombre) as local,
     upper(CNT.valor) as tipo_direccion,
     upper(LOC.direccion) as direccion,
     LOC.nro,     
     upper(LOC.mazana) as manzana,
     LOC.lote
